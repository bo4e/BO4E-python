    "description": "Modell zur Abbildung eines kompakten Lastganges.\nIn diesem Modell werden die Messwerte in Form von Tagesvektoren mit fester Anzahl von Werten übertragen.\nDaher ist dieses BO nur zur Übertragung von äquidistanten Messwertverläufen geeignet.",
    "properties": {
        "boTyp": {
            "allOf": [
                {
                    "$ref": "#/definitions/BoTyp"
                }
            ],
            "default": "LASTGANG_KOMPAKT"
        },
        "externeReferenzen": {
            "default": [],
            "items": {
                "$ref": "#/definitions/ExterneReferenz"
            },
            "title": "Externereferenzen",
            "type": "array"
        },
        "lokationsId": {
            "title": "Lokationsid",
            "type": "string"
        },
        "lokationstyp": {
            "title": "Lokationstyp",
            "type": "string"
        },
        "messgroesse": {
            "$ref": "#/definitions/Mengeneinheit"
        },
        "obisKennzahl": {
            "pattern": "((1)-((?:[0-5]?[0-9])|(?:6[0-5])):((?:[1-8]|99))\\.((?:6|8|9|29))\\.([0-9]{1,2})|(7)-((?:[0-5]?[0-9])|(?:6[0-5])):(.{1,2})\\.(.{1,2})\\.([0-9]{1,2}))",
            "title": "Obiskennzahl",
            "type": "string"
        },
        "sparte": {
            "$ref": "#/definitions/Sparte"
        },
        "tagesvektoren": {
            "items": {
                "$ref": "#/definitions/Tagesvektor"
            },
            "title": "Tagesvektoren",
            "type": "array"
        },
        "version": {
            "title": "Version",
            "type": "string"
        },
        "versionstruktur": {
            "default": "2",
            "title": "Versionstruktur",
            "type": "string"
        },
        "zeitintervall": {
            "$ref": "#/definitions/Zeitintervall"
        }
    },
    "required": [
        "sparte",
        "lokationsId",
        "lokationstyp",
        "messgroesse",
        "zeitintervall",
        "tagesvektoren"
    ],
    "title": "LastgangKompakt",
    "type": "object"
