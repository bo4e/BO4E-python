              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": [],
          "title": "Kontaktweg"
        },
        "umsatzsteuerId": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
