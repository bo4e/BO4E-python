        "hrnummer": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Hrnummer"
        },
        "amtsgericht": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
