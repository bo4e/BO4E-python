    "Energieherkunft": {
      "additionalProperties": true,
      "description": "Abbildung einer Energieherkunft\n\n.. raw:: html\n\n    <object data=\"../_static/images/bo4e/com/Energieherkunft.svg\" type=\"image/svg+xml\"></object>\n\n.. HINT::\n    `Energieherkunft JSON Schema <https://json-schema.app/view/%23?url=https://raw.githubusercontent.com/Hochfrequenz/BO4E-python/main/json_schemas/com/Energieherkunft.json>`_",
      "properties": {
        "erzeugungsart": {
          "$ref": "#/$defs/Erzeugungsart"
        },
        "anteilProzent": {
          "anyOf": [
            {
              "exclusiveMaximum": 100.0,
              "exclusiveMinimum": 0.0,
              "type": "number"
            },
            {
              "type": "string"
            }
          ],
          "title": "Anteilprozent"
        }
      },
      "required": ["erzeugungsart", "anteilProzent"],
      "title": "Energieherkunft",
      "type": "object"
    },
    "Energiemix": {
      "additionalProperties": true,
      "description": "Zusammensetzung der gelieferten Energie aus den verschiedenen Prim\u00e4renergieformen.\n\n.. raw:: html\n\n    <object data=\"../_static/images/bo4e/com/Energiemix.svg\" type=\"image/svg+xml\"></object>\n\n.. HINT::\n    `Energiemix JSON Schema <https://json-schema.app/view/%23?url=https://raw.githubusercontent.com/Hochfrequenz/BO4E-python/main/json_schemas/com/Energiemix.json>`_",
      "properties": {
        "energiemixnummer": {
          "title": "Energiemixnummer",
          "type": "integer"
        },
        "energieart": {
          "$ref": "#/$defs/Sparte"
        },
        "bezeichnung": {
          "title": "Bezeichnung",
          "type": "string"
        },
        "gueltigkeitsjahr": {
          "title": "Gueltigkeitsjahr",
          "type": "integer"
        },
        "anteil": {
          "items": {
            "$ref": "#/$defs/Energieherkunft"
          },
          "minItems": 1,
          "title": "Anteil",
          "type": "array"
        },
        "bemerkung": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Bemerkung"
        },
        "co2Emission": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Co2Emission"
        },
        "atommuell": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Atommuell"
        },
        "oekozertifikate": {
          "default": [],
          "items": {
            "$ref": "#/$defs/Oekozertifikat"
          },
          "title": "Oekozertifikate",
          "type": "array"
        },
        "oekolabel": {
          "default": [],
          "items": {
            "$ref": "#/$defs/Oekolabel"
          },
          "title": "Oekolabel",
          "type": "array"
        },
        "oekoTopTen": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Oekotopten"
        },
        "website": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Website"
        }
      },
      "required": [
        "energiemixnummer",
        "energieart",
        "bezeichnung",
        "gueltigkeitsjahr",
        "anteil"
      ],
      "title": "Energiemix",
      "type": "object"
    },
