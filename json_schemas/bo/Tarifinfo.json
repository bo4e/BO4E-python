{
  "$defs": {
    "Adresse": {
      "additionalProperties": true,
      "description": "Contains an address that can be used for most purposes.\n\n.. raw:: html\n\n    <object data=\"../_static/images/bo4e/com/Adresse.svg\" type=\"image/svg+xml\"></object>\n\n.. HINT::\n    `Adresse JSON Schema <https://json-schema.app/view/%23?url=https://raw.githubusercontent.com/Hochfrequenz/BO4E-python/main/json_schemas/com/Adresse.json>`_",
      "properties": {
        "postleitzahl": {
          "title": "Postleitzahl",
          "type": "string"
        },
        "ort": {
          "title": "Ort",
          "type": "string"
        },
        "ortsteil": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Ortsteil"
        },
        "strasse": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Strasse"
        },
        "hausnummer": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Hausnummer"
        },
        "postfach": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Postfach"
        },
        "adresszusatz": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Adresszusatz"
        },
        "coErgaenzung": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Coergaenzung"
        },
        "landescode": {
          "allOf": [
            {
              "$ref": "#/$defs/Landescode"
            }
          ],
          "default": "DE"
        }
      },
      "required": ["postleitzahl", "ort"],
      "title": "Adresse",
      "type": "object"
    },
    "Anrede": {
      "description": "\u00dcbersicht m\u00f6glicher Anreden, z.B. eines Gesch\u00e4ftspartners.",
