        "LASTGANG_KOMPAKT",
        "MARKTLOKATION",
        "MESSLOKATION",
        "MARKTTEILNEHMER",
        "NETZNUTZUNGSRECHNUNG",
        "PREISBLATT",
        "PREISBLATTDIENSTLEISTUNG",
        "PREISBLATTHARDWARE",
        "PREISBLATTKONZESSIONSABGABE",
        "PREISBLATTMESSUNG",
        "PREISBLATTNETZNUTZUNG",
        "PREISBLATTUMLAGEN",
        "RECHNUNG",
        "REGION",
        "REGIONALTARIF",
