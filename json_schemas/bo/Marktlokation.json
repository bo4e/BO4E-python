        "Geschaeftspartner": {
            "description": "Mit diesem Objekt können Geschäftspartner übertragen werden.\nSowohl Unternehmen, als auch Privatpersonen können Geschäftspartner sein.\nHinweis: Marktteilnehmer haben ein eigenes BO, welches sich von diesem BO ableitet.\nHier sollte daher keine Zuordnung zu Marktrollen erfolgen.\n\n.. graphviz:: /api/dots/bo4e/bo/Geschaeftspartner.dot",
            "properties": {
                "amtsgericht": {
                    "title": "Amtsgericht",
                    "type": "string"
                },
                "anrede": {
                    "$ref": "#/definitions/Anrede"
                },
                "boTyp": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/BoTyp"
                        }
                    ],
                    "default": "GESCHAEFTSPARTNER"
                },
                "eMailAdresse": {
                    "title": "Emailadresse",
                    "type": "string"
                },
                "externeReferenzen": {
                    "default": [],
                    "items": {
                        "$ref": "#/definitions/ExterneReferenz"
                    },
                    "title": "Externereferenzen",
                    "type": "array"
                },
                "geschaeftspartnerrolle": {
                    "items": {
                        "$ref": "#/definitions/Geschaeftspartnerrolle"
                    },
                    "type": "array"
                },
                "gewerbekennzeichnung": {
                    "title": "Gewerbekennzeichnung",
                    "type": "boolean"
                },
                "glaeubigerId": {
                    "title": "Glaeubigerid",
                    "type": "string"
                },
                "hrnummer": {
                    "title": "Hrnummer",
                    "type": "string"
                },
                "kontaktweg": {
                    "default": [],
                    "items": {
                        "$ref": "#/definitions/Kontaktart"
                    },
                    "type": "array"
                },
                "name1": {
                    "title": "Name1",
                    "type": "string"
                },
                "name2": {
                    "title": "Name2",
                    "type": "string"
                },
                "name3": {
                    "title": "Name3",
                    "type": "string"
                },
                "partneradresse": {
                    "$ref": "#/definitions/Adresse"
                },
                "umsatzsteuerId": {
                    "title": "Umsatzsteuerid",
                    "type": "string"
                },
                "versionstruktur": {
                    "default": "2",
