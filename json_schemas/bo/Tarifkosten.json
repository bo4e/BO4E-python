                "bis": {
                    "format": "date-time",
                    "title": "Bis",
                    "type": "string"
                },
                "einzelpreis": {
                    "$ref": "#/definitions/Preis"
                },
                "menge": {
                    "$ref": "#/definitions/Menge"
                },
                "positionstitel": {
                    "title": "Positionstitel",
                    "type": "string"
                },
                "von": {
                    "format": "date-time",
                    "title": "Von",
                    "type": "string"
                },
                "zeitmenge": {
                    "$ref": "#/definitions/Menge"
                }
            },
            "required": [
                "positionstitel",
                "betragKostenposition",
                "artikelbezeichnung",
                "einzelpreis"
            ],
            "title": "Kostenposition",
            "type": "object"
