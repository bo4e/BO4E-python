  "$defs": {
    "Adresse": {
      "additionalProperties": true,
      "description": "\n    Contains an address that can be used for most purposes.\n\n    .. raw:: html\n\n        <object data=\"../_static/images/bo4e/com/Adresse.svg\" type=\"image/svg+xml\"></object>\n\n    .. HINT::\n        `Adresse JSON Schema <https://json-schema.app/view/%23?url=https://raw.githubusercontent.com/Hochfrequenz/BO4E-python/main/json_schemas/com/Adresse.json>`_\n\n    ",
      "properties": {
        "adresszusatz": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Adresszusatz"
        },
        "coErgaenzung": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Coergaenzung"
        },
        "hausnummer": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Hausnummer"
        },
        "landescode": {
          "allOf": [
            {
              "$ref": "#/$defs/Landescode"
            }
          ],
          "default": "DE"
        },
        "ort": {
          "title": "Ort",
          "type": "string"
        },
        "ortsteil": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Ortsteil"
        },
        "postfach": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Postfach"
        },
        "postleitzahl": {
          "title": "Postleitzahl",
          "type": "string"
        },
        "strasse": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Strasse"
        }
      },
      "required": ["postleitzahl", "ort"],
      "title": "Adresse",
      "type": "object"
    },
