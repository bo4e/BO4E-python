        },
        {
          "type": "null"
        }
      ],
      "default": null
    },
    "positionspreis": {
      "$ref": "#/$defs/Preis"
    }
  },
  "required": ["positionsbezeichnung", "positionspreis"],
  "title": "Angebotsposition",
  "type": "object"
