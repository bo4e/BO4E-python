            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Dauer"
        },
        "startdatum": {
          "anyOf": [
            {
              "format": "date-time",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Startdatum"
        },
        "enddatum": {
          "anyOf": [
            {
              "format": "date-time",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
