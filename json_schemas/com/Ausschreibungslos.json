                    "title": "Enddatum",
                    "type": "string"
                },
                "endzeitpunkt": {
                    "format": "date-time",
                    "title": "Endzeitpunkt",
                    "type": "string"
                },
                "startdatum": {
                    "format": "date-time",
                    "title": "Startdatum",
                    "type": "string"
                },
                "startzeitpunkt": {
                    "format": "date-time",
                    "title": "Startzeitpunkt",
                    "type": "string"
                }
            },
            "title": "Zeitraum",
            "type": "object"
        }
