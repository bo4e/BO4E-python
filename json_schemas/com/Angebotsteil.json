                    "$ref": "#/definitions/Menge"
                },
                "positionspreis": {
                    "$ref": "#/definitions/Preis"
                }
            },
            "required": [
                "positionsbezeichnung",
                "positionspreis"
            ],
            "title": "Angebotsposition",
            "type": "object"
