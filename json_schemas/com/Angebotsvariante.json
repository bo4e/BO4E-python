      "title": "Geokoordinaten",
      "type": "object"
    },
    "Geschaeftspartner": {
      "additionalProperties": true,
      "description": "Mit diesem Objekt k\u00f6nnen Gesch\u00e4ftspartner \u00fcbertragen werden.\nSowohl Unternehmen, als auch Privatpersonen k\u00f6nnen Gesch\u00e4ftspartner sein.\nHinweis: Marktteilnehmer haben ein eigenes BO, welches sich von diesem BO ableitet.\nHier sollte daher keine Zuordnung zu Marktrollen erfolgen.\n\n.. raw:: html\n\n    <object data=\"../_static/images/bo4e/bo/Geschaeftspartner.svg\" type=\"image/svg+xml\"></object>\n\n.. HINT::\n    `Geschaeftspartner JSON Schema <https://json-schema.app/view/%23?url=https://raw.githubusercontent.com/Hochfrequenz/BO4E-python/main/json_schemas/bo/Geschaeftspartner.json>`_",
      "properties": {
        "versionstruktur": {
          "default": "2",
          "title": "Versionstruktur",
          "type": "string"
        },
        "boTyp": {
          "allOf": [
            {
              "$ref": "#/$defs/BoTyp"
            }
          ],
          "default": "GESCHAEFTSPARTNER"
        },
        "externeReferenzen": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/ExterneReferenz"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": [],
          "title": "Externereferenzen"
        },
        "name1": {
          "title": "Name1",
          "type": "string"
        },
        "gewerbekennzeichnung": {
          "title": "Gewerbekennzeichnung",
          "type": "boolean"
        },
        "geschaeftspartnerrolle": {
          "items": {
            "$ref": "#/$defs/Geschaeftspartnerrolle"
          },
          "title": "Geschaeftspartnerrolle",
          "type": "array"
        },
        "anrede": {
          "anyOf": [
            {
              "$ref": "#/$defs/Anrede"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        },
        "name2": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name2"
        },
        "name3": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name3"
        },
        "hrnummer": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Hrnummer"
        },
        "amtsgericht": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Amtsgericht"
        },
        "kontaktweg": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/Kontaktart"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": [],
          "title": "Kontaktweg"
        },
        "umsatzsteuerId": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Umsatzsteuerid"
        },
        "glaeubigerId": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Glaeubigerid"
        },
        "eMailAdresse": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Emailadresse"
        },
        "website": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Website"
        },
        "partneradresse": {
          "anyOf": [
            {
              "$ref": "#/$defs/Adresse"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        }
      },
      "required": ["name1", "gewerbekennzeichnung", "geschaeftspartnerrolle"],
      "title": "Geschaeftspartner",
      "type": "object"
    },
